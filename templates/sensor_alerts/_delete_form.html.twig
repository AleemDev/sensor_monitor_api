<form method="post" action="{{ path('sensor_alerts_delete', {'id': sensor_alert.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ sensor_alert.id) }}">
    <button class="btn">Delete</button>
</form>
